{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "es6",
      "dom",
    ],
    "moduleResolution": "node",
    "module": "esnext",
    "sourceMap": true,
    "outDir": "./build",
    "downlevelIteration": true
  },
  "exclude": [
    "./node_modules"
  ],
  "include": [
    "/src/*",
    "./libs/**/*"
  ]
}
